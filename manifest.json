{
  "manifest_version": 2,

  "name": "dweetio",
  "description": "simple dweet.io client",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
	"storage",
    "https://ajax.googleapis.com/",
	"https://dweet.io/"
  ]
}
